<div class="container">
  <div class="address_edit">
    <h3>配送先編集</h3></br>
    <%= form_with model: @address, url: users_address_path(@address), local: true do |f| %>
    <p>
      氏名：
      <%= f.text_field :name, :size => "15" %>　
      郵便番号：
      <%= f.text_field :postal_code, :size => "10" %>　
      住所：
      <%= f.text_field :address, :size => "40" %>　
      TEL：　
      <%= f.text_field :telephone, :size => "15" %>
      <%= f.submit "更新", class: "btn btn-warning" %>
    </p>
    <% end %>
  </div>
</div>
